{
    "name": "hamedsz/laravel-todo",
    "authors": [
        {
            "name": "hamedsz",
            "email": "badmansog@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "^7.0"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "orchestra/testbench": "5.x",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "TodoApp\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TodoApp\\Tests\\": "./tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TodoApp\\TodoServiceProvider"
            ]
        }
    },
    "scripts": {
        "phpunit": "vendor/bin/phpunit"
    }
}
